displayoptions nmbase
iftype athsta
athsta_base wifi0
addressing none
wpa_ssid [none]
wpa_key_mgmt  WPA-EAP
wpa_identity username@domain
wpa_password password
wpa_proto  WPA
wpa_pairwise CCMP
wpa_group CCMP
wpa_eap TLS
wpa_client_cert  /storage/cert.der
wpa_private_key  /storage/key.der
wpa_private_key_passwd
wpa_driver madwifi

